<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="genomeCATPU" transaction-type="RESOURCE_LOCAL">
    <provider>oracle.toplink.essentials.ejb.cmp3.EntityManagerFactoryProvider</provider>
    <class>org.molgen.genomeCATPro.datadb.dbentities.AnnotationList</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.Study</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.User</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.ExperimentDetail</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.PlatformDetail</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.SampleInExperiment</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.ExperimentData</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.PlatformData</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.SampleDetail</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.Track</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.MapData</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.MapDetail</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.ExperimentAtStudy</class>
    <!--<class>org.molgen.genomeCATPro.peaks.cnvcat.AberrationCNVCAT</class>-->
    <class>org.molgen.genomeCATPro.datadb.dbentities.TrackAtStudy</class>
    <class>org.molgen.genomeCATPro.datadb.dbentities.SampleInTrack</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="persistenceXmlLocation" value="classpath:META-INF/persistence.xml"/>
      <property name="toplink.jdbc.user" value="user"/>
      <property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306"/>
      <property name="toplink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="toplink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
